{
  "name": "todo-plumejs",
  "version": "1.0.0",
  "description": "a plumejs and trpc based todo application",
  "main": "index.js",
  "scripts": {
    "start": "env-cmd -f ./.env vite",
    "build": "vite build",
    "tsc": "tsc -p tsconfig.json --noEmit",
    "test": "vitest",
    "test:coverage": "yarn test --coverage",
    "lint": "eslint . --ext .ts --fix",
    "show-dep-graph": "skott --displayMode=webapp",
    "generate:codegen": "openapi-ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@plumejs/core": "^4.1.9",
    "@plumejs/testing": "^4.1.1",
    "@trpc/client": "^11.0.0-rc.417",
    "@trpc/server": "^11.0.0-rc.417",
    "env-cmd": "^10.1.0"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.47.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.77.6",
    "skott": "^0.35.0",
    "typescript": "^5.5.2",
    "vite": "^5.3.1",
    "vitest": "^1.6.0"
  }
}
